{
    "name": "svelte-gantt",
    "description": "Interactive JavaScript Gantt chart/resource booking component",
    "version": "4.0.3-beta",
    "author": "Ante Novokmet <ante.je.zakon@gmail.com>",
    "scripts": {
        "build": "rollup -c",
        "autobuild": "rollup -c -w",
        "dev": "run-p start autobuild",
        "demo:autobuild": "cd demo && npm run autobuild",
        "demo:start": "sirv docs --dev -s",
        "demo:dev": "run-p demo:start demo:autobuild",
        "docs": "run-p docs:*",
        "validate": "svelte-check"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/ANovokmet/svelte-gantt.git"
    },
    "bugs": {
        "url": "https://github.com/ANovokmet/svelte-gantt/issues"
    },
    "keywords": [
        "svelte",
        "gantt",
        "booking",
        "svelte-gantt",
        "js-gantt",
        "javascript-gantt",
        "react-gantt",
        "angular-gantt",
        "vue-gantt",
        "drag-n-drop"
    ],
    "types": "types/index.d.ts",
    "main": "index.js",
    "peerDependencies": {
        "moment": "^2.22.2"
    },
    "devDependencies": {
        "@rollup/plugin-commonjs": "^17.1.0",
        "@rollup/plugin-typescript": "^8.3.2",
        "@tsconfig/svelte": "^1.0.13",
        "del": "^5.1.0",
        "npm-run-all": "^4.1.5",
        "postcss": "^8.4.14",
        "rollup": "^2.75.5",
        "rollup-plugin-postcss": "^4.0.2",
        "rollup-plugin-svelte": "^5.2.3",
        "rollup-plugin-typescript2": "^0.22.1",
        "rollup-plugin-uglify": "^3.0.0",
        "svelte": "^3.48.0",
        "svelte-check": "^1.6.0",
        "svelte-preprocess": "^4.10.6",
        "svelte-ts-preprocess": "^1.1.3",
        "tslib": "^2.4.0",
        "typescript": "^4.7.2"
    },
    "dependencies": {
        "@rollup/plugin-buble": "^0.21.3",
        "@rollup/plugin-node-resolve": "^13.3.0",
        "@sveltejs/kit": "1.0.0-next.347",
        "sirv-cli": "^1.0.14",
        "uglify-js": "^3.15.5"
    }
}
